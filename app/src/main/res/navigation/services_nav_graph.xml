            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/fragmentAwards"
        android:name="com.company.metrix.ui.servicesEmployee.awards.FragmentAwards"
        android:label="FragmentAwards" >
