<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/services_nav_graph"
    app:startDestination="@id/service_fragment">

    <fragment
        android:id="@+id/service_fragment"
        android:name="com.company.metrix.ui.servicesEmployee.ServicesFragment"
        android:label="ServiceFragment" >
        <action
            android:id="@+id/action_service_fragment_to_fragmentAwards"
            app:destination="@id/fragmentAwards"
            app:enterAnim="@anim/slide_enter_anim"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_service_fragment_to_fragmentDiagnostic"
            app:destination="@id/fragmentDiagnostic"
            app:enterAnim="@anim/slide_enter_anim"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_service_fragment_to_fragmentTeam"
            app:destination="@id/fragmentTeam"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@android:anim/slide_in_left"
            app:popExitAnim="@android:anim/slide_out_right" />
        <action
            android:id="@+id/action_service_fragment_to_fragmentRating"
            app:destination="@id/fragmentRating"
            app:enterAnim="@anim/slide_enter_anim"
